{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/advanced-posts",
	"version": "0.1.0",
	"title": "Advanced Posts",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "advanced-posts",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"postType": {
			"type": "string",
			"default": ""
		},
		"taxonomy": {
			"type": "string",
			"default": ""
		},
		"terms": {
			"type": "array",
			"default": [],
			"items": {
				"type": "integer"
			}
		}
	}
	
}
